<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0"
            prefWidth="500.0"
            xmlns="http://javafx.com/javafx/11.0.14-internal"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="dp.wgu.softwareii.controller.AddCustomerPageController">
   <children>
      <VBox layoutX="25.0" layoutY="25.0" maxWidth="-Infinity" prefHeight="450.0" prefWidth="450.0" spacing="30.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="550.0" spacing="60.0">
               <children>
                  <Label text="Add Customer">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <GridPane hgap="20.0" prefHeight="300.0" prefWidth="550.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="162.0" minWidth="10.0" prefWidth="124.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="262.0" minWidth="10.0" prefWidth="177.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="ID" />
                  <Label text="Name" GridPane.rowIndex="1" />
                  <Label text="Address" GridPane.rowIndex="2" />
                  <Label prefHeight="25.0" prefWidth="96.0" text="Postal Code" GridPane.rowIndex="3" />
                  <Label text="Phone Number" GridPane.rowIndex="4" />
                  <Label fx:id="partTypeLabel" text="Country" GridPane.rowIndex="5" />
                  <TextField disable="true" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" prefWidth="162.0" promptText="Auto Gen - Disabled" GridPane.columnIndex="1" />
                  <TextField fx:id="nameField" layoutX="71.0" layoutY="15.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="addressField" layoutX="71.0" layoutY="50.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="postalField" layoutX="71.0" layoutY="85.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="phoneField" layoutX="71.0" layoutY="120.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="partTypeLabel1" layoutX="10.0" layoutY="234.0" text="State/Province" GridPane.rowIndex="6" />
                  <ComboBox fx:id="countryComboBox" promptText="Combo Box 1" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <ComboBox fx:id="stateComboBox" promptText="Combo Box 2" GridPane.columnIndex="1" GridPane.rowIndex="6" />
               </children>
            </GridPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button fx:id="AddButton" mnemonicParsing="false" onAction="#OnSaveClick" prefWidth="70.0" text="Save" />
                  <Button fx:id="CancelBtn" mnemonicParsing="false" onAction="#OnCancelClick" prefWidth="70.0" text="Cancel" />
               </children>
            </HBox>
         </children></VBox>
   </children>
</AnchorPane>
