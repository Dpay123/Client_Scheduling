<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dp.wgu.softwareii.controller.LoginPageController">
   <children>
      <VBox alignment="CENTER" prefHeight="300.0" prefWidth="300.0" spacing="15.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="43.0" prefWidth="250.0">
               <children>
                  <Label alignment="TOP_CENTER" text="Application Login" underline="true">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font></Label>
               </children>
            </HBox>
            <GridPane alignment="CENTER" hgap="20.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="10.0" prefWidth="78.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="172.0" minWidth="10.0" prefWidth="172.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Username:" GridPane.rowIndex="1" />
                  <Label text="Password:" GridPane.rowIndex="2" />
                  <TextField fx:id="userField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="32.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                     <children>
                        <Button fx:id="loginButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onLoginClick" prefWidth="70.0" text="Login" />
                        <Button fx:id="exitButton" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onExitClick" prefWidth="70.0" text="Exit" />
                     </children>
                  </HBox>
                  <Label text="Time:" />
                  <Label fx:id="loginTime" text="TimeObjGoesHere" GridPane.columnIndex="1" />
               </children>
            </GridPane>
         </children>
         <padding>
            <Insets left="15.0" right="15.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
